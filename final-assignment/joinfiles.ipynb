   "source": [
    "for file in filesinthatfolder:\n",
    "    if file[-4:] != \".csv\":\n",
    "        continue\n",
    "    \n",
    "    # Open in write mode the file\n",
    "    with open(file, \"r\") as f:\n",
    "        lines = f.read().splitlines(True)\n",
    "    \n",
    "    with open(file, 'w') as f:\n",
    "        f.writelines(lines[1:])"
   ]
  }
